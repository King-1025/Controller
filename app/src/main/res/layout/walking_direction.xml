<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	android:visibility="visible">

	<king.helper.view.FloatingLayout
		android:layout_height="match_parent"
		android:layout_width="match_parent"
		android:visibility="visible">

		<king.helper.view.CircularRod
			android:layout_width="@dimen/walking_circularrod_size"
			android:layout_height="@dimen/walking_circularrod_size"
			app:bgInnerDrawable="@drawable/sel_inner_circle"
			app:bgOuterDrawable="@drawable/sel_outer_circle"
			android:layout_alignParentBottom="true"
			android:layout_marginLeft="@dimen/walking_circularrod_margin_left"
			android:layout_marginBottom="@dimen/walking_circularrod_margin_bottom"
			android:id="@+id/walkingdirectionCircularRod"
			android:visibility="visible"/>

	</king.helper.view.FloatingLayout>

</LinearLayout>

